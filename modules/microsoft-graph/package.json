{
  "name": "microsoft-graph",
  "version": "1.0.0",
  "description": "Integrates Microsoft Graph with Cognigy.AI",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p .",
    "zip": "zip microsoftgraph.zip build/* package.json package-lock.json README.md icon.png icon-large.png",
    "build": "npm run transpile && npm run lint && npm run zip",
    "lint": "tslint -c tslint.json src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cognigy/CustomModules.git"
  },
  "keywords": [
    "Microsoft",
    "Microsoft",
    "Graph",
    "Graph",
    "API"
  ],
  "author": "Cognigy GmbH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cognigy/CustomModules/issues"
  },
  "homepage": "https://github.com/Cognigy/CustomModules#readme",
  "devDependencies": {
    "@types/node": "^10.14.6",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^1.7.0"
  }
}
